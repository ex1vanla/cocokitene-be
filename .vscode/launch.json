        {
            "type": "node",
            "request": "launch",
            "name": "Debug Cronjob Cocokitene",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "console:dev", "--", "--inspect-brk"],
            "autoAttachChildProcesses": true,
            "restart": true,
            "sourceMaps": true,
            "stopOnEntry": false,
            "console": "integratedTerminal"
        }
    ]
}
